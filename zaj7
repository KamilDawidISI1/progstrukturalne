#include <stdio.h>
#include <stdlib.h>

void wypisz(unsigned int n, int * tab){
    int i;
    for(i=0;i<n;i++){
        printf("%d\t", tab[i]);
    }
    printf("\n");

}

void suma(unsigned int n, int *tab1, int *tab2, int *tab3){
    int i;
    for(i=0; i<n; i++){
        tab3[i] = tab1[i]+tab2[i];
    }
    wypisz(n, tab3);
}

void max(unsigned int n, int *tab1, int *tab2, int *tab3){
    int i;
    for(i=0; i<n; i++){
        if(tab1[i]>tab2[i]){
            tab3[i] = tab1[i];
        }else{
            tab3[i] = tab2[i];
        }
    }
    wypisz(n, tab3);
}

void karuzela(unsigned int n, int *tab1, int *tab2, int *tab3){
    int temp;
    int i;
    for(i=0;i<n;i++){
        temp = tab2[i];
        tab2[i] = tab1[i];
        tab1[i] = tab3[i];
        tab3[i] = temp;
    }    
}

int main()
{
    int tab1[] ={1,2,3,4,5};
    int tab2[] ={4,3,2,7,3};
    int tab3[] =[3,5,3,2,1];
    int n = 5;
    suma(n ,tab1, tab2, tab3);
    max(n ,tab1, tab2, tab3);
    karuzela(n ,tab1, tab2, tab3);
    return 0;
}


-----------------------------------------------------

#include <stdio.h>
#include <stdlib.h>

void wypisz(unsigned int n, double * tab){
    int i;
    for(i=0;i<n;i++){
        printf("%f\t", tab[i]);
    }
    printf("\n");

}

void doklejenie(unsigned int n, double *tab1, double *tab2, double *tab3){
    int i;
    for(i=0;i<2*n;i++){
        if(i<n){
            tab3[i] = tab1[i];
        }else{
            tab3[i] = tab2[i-n];
        }
    }
    wypisz(2*n, tab3);
}

void doklejenie2(unsigned int n, double *tab1, double *tab2, double *tab3){
    int i;
    for(i=0;i<2*n;i++){
        if(i%2==0){
            tab3[i] = tab2[i/2];
        }else{
            tab3[i] = tab1[(i-1)/2];
        }
    }
    wypisz(2*n, tab3);
}



int main()
{
    double tab[] = {2,7,1,3};
    double tabb[] = {4,5,2,8};
    double tabbb[8];
    doklejenie(4, tab, tabb, tabbb);
    doklejenie2(4, tab, tabb, tabbb);
    return 0;
}
